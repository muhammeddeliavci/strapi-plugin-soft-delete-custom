    "react-router-dom": "^6.0.0"
  },
  "devDependencies": {
    "@strapi/strapi": "^5.34.0",
    "@strapi/typescript-utils": "^5.34.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@strapi/design-system": "^2.0.0",
    "@strapi/icons": "^2.0.0"
