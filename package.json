{
  "name": "@siyahbeyaz/strapi-plugin-soft-delete-custom",
  "version": "1.2.2",
  "description": "Strapi v5 plugin for soft delete functionality",
  "type": "module",
  "main": "./dist/strapi-server.mjs",
  "exports": {
    "./strapi-server": {
      "import": "./dist/strapi-server.mjs",
      "require": "./dist/strapi-server.cjs"
    },
    "./strapi-admin": {
      "import": "./dist/strapi-admin.mjs",
      "require": "./dist/strapi-admin.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "scripts",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "build:clean": "rimraf dist && tsup",
    "validate": "node scripts/validate-build.js",
    "prepublishOnly": "npm run build:clean",
    "postinstall": "node scripts/postinstall.cjs || exit 0",
    "setup": "node scripts/setup.cjs"
  },
  "dependencies": {
    "react-dnd": "^16.0.0",
    "react-dnd-html5-backend": "^16.0.0"
  },
  "devDependencies": {
    "@strapi/design-system": "^2.0.0",
    "@strapi/icons": "^2.0.0",
    "@strapi/strapi": "^5.35.0",
    "@types/node": "^18.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.0",
    "rimraf": "^5.0.10",
    "styled-components": "^6.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@strapi/strapi": "^5.35.0"
  },
  "engines": {
    "node": ">=18.0.0 <=22.x.x",
    "npm": ">=6.0.0"
  },
  "strapi": {
    "name": "soft-delete",
    "displayName": "Soft Delete",
    "kind": "plugin",
    "supportedVersions": ">=5.35.0 <6.0.0"
  },
  "keywords": [
    "strapi",
    "plugin",
    "soft-delete",
    "trash",
    "recycle-bin"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muhammeddeliavci/strapi-plugin-soft-delete-custom.git"
  },
  "bugs": {
    "url": "https://github.com/muhammeddeliavci/strapi-plugin-soft-delete-custom/issues"
  },
  "homepage": "https://github.com/muhammeddeliavci/strapi-plugin-soft-delete-custom#readme"
}
