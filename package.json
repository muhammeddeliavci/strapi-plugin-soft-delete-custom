    "minimumStrapiVersion": "5.34.0"
  },
  "keywords": [
    "strapi",
    "plugin",
    "soft-delete",
    "restore",
    "trash",
    "recycle-bin",
    "strapi-plugin",
    "strapi-v5",
    "cms",
    "content-management"
  ],
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/strapi-plugin-soft-delete-custom"
  },
  "bugs": {
    "url": "https://github.com/yourusername/strapi-plugin-soft-delete-custom/issues"
  },
  "homepage": "https://github.com/yourusername/strapi-plugin-soft-delete-custom#readme",
  "license": "MIT",
  "main": "./src/index.ts",
  "exports": {
    "./strapi-admin": {
      "source": "./src/admin/index.ts",
      "default": "./src/admin/index.ts"
    },
    "./strapi-server": {
      "source": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "strapi-plugin build",
