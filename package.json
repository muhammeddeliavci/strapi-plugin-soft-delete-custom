{
  "name": "strapi-plugin-soft-delete-custom",
  "version": "1.0.1",
  "description": "Strapi v5 soft delete plugin with restore functionality",
  "files": [
    "dist/",
    "src/",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "strapi": {
